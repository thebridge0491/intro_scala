      <includes> <include>**/*.txt</include> <include>**/*.xml</include>
        <include>**/*.gradle</include> <include>**/*.mk</include>
        <include>**/*.rb</include> <include>**/*.new</include>
        <include>**/*.sbt</include> <include>**/*.</include>
         <include>**/gradle.properties</include> </includes>
      <excludes> <exclude>**/depnmgr-ivy.xml</exclude> 
        <exclude>**/depnmgr-maven.xml*</exclude> 
        <exclude>**/build-jacoco.xml*</exclude> </excludes>
    </fileSet>
    <fileSet filtered="true" encoding="UTF-8">
      <directory>choices/build_tool</directory>
      <includes> <include>**/*.txt</include> <include>**/*.xml</include>
        <include>**/*.gradle</include> <include>**/*.mk</include>
        <include>**/*.rb</include> <include>**/*.new</include>
        <include>**/*.sbt</include> <include>**/*.bak</include>
         <include>**/gradle.properties</include> </includes>
