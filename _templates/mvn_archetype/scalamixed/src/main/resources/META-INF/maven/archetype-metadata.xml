        <exclude>**/rakefile-targets.rb</exclude>  
        <exclude>**/Makefile-targets.mk</exclude>  
        <exclude>**/build-targets.gradle</exclude> </excludes>
    </fileSet>
    <fileSet filtered="true" encoding="UTF-8">
      <directory>choices/ffi_lib</directory>
      <includes> <include>**/*.scala</include> <include>**/*.java</include>
        <include>**/*.i</include> </includes>
    </fileSet>
    <fileSet filtered="true" encoding="UTF-8">
